<view class="pages"><view class="header_box"><view data-event-opts="{{[['tap',[['goBack']]]]}}" class="header_back" bindtap="__e"><image src="{{$root.m0}}" data-event-opts="{{[['tap',[['goBack']]]]}}" bindtap="__e" class="_img"></image></view><view class="header_all"><text>今日跟进</text></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="header_search" bindtap="__e">筛选</view></view><view class="page-search"><block wx:if="{{isShowPageNav}}"><view data-event-opts="{{[['tap',[['closePageNavFn',['$event']]]]]}}" class="allcusList" bindtap="__e"><view class="allcusList_box"><block wx:for="{{pageNav}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['PageNavFn',[index]]]]]}}" class="{{['allcusList_main',pageNavIndex==index?'activeallcusList_main':'']}}" catchtap="__e"><text>{{item}}</text><block wx:if="{{pageNavIndex==index}}"><text>✓</text></block></view></block></view></view></block><view class="topmuie"><view class="seek"><input type="text" placeholder-style="font-size:13px" data-key="keywords" placeholder="请输入手机号进行查询" data-event-opts="{{[['input',[['inputChange',['$event']]]]]}}" value="{{keywords}}" bindinput="__e"/><image src="{{$root.m1}}" data-event-opts="{{[['tap',[['search',['$event']]]]]}}" bindtap="__e" class="_img"></image></view></view><block wx:if="{{isShowSearchCompontent}}"><view class="topmuieFixed"><view class="topmuieFixed_main"><view class="topmuieFixed_box"><view class="searchNavBar"><block wx:for="{{searchNavBar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['searchNav',[index]]]]]}}" class="{{[searchNavIndex===index?'searchNavBartext':'']}}" catchtap="__e">{{item}}</text></block></view><view class="topmuieFixed_right"><block wx:if="{{searchItemsFields!=''}}"><block wx:for="{{searchItemsFields}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['searchNavItemClick',[index]]]]]}}" class="{{[searchSelectedItemIndex===index?'searchItemsFields':'']}}" catchtap="__e">{{item.title}}</text></block></block><block wx:if="{{searchNavIndex===3||searchNavIndex===4||searchNavIndex===5||searchNavIndex===6}}"><block wx:for="{{searchDateTextItems}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['searchNavItemClick',[index]]]]]}}" class="{{[searchSelectedItemIndex===index?'searchItemsFields':'']}}" catchtap="__e">{{item}}</text></block></block></view></view><block wx:if="{{searchNavIndex>2&&searchSelectedItemIndex===searchDateTextItems.length-1}}"><view class="clocedtime"><picker mode="date" value="{{startDate}}" data-event-opts="{{[['change',[['startDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input"><text>{{"开始时间:"+startDate}}</text></view></picker><picker mode="date" value="{{endDate}}" data-event-opts="{{[['change',[['endDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input"><text>{{"结束时间:"+endDate}}</text></view></picker></view></block><view class="topmuieFixed_setting"><text data-event-opts="{{[['tap',[['emptyFn',['$event']]]]]}}" catchtap="__e">清空</text><text data-event-opts="{{[['tap',[['makeSure',['$event']]]]]}}" catchtap="__e">确定</text></view></view></view></block></view><block wx:for="{{customers}}" wx:for-item="customer" wx:for-index="index" wx:key="id"><view class="msg"><text class="msgtopright">{{index+1}}</text><view data-event-opts="{{[['tap',[['navToCustomer',['$0'],[[['customers','id',customer.id,'member_id']]]]]]]}}" bindtap="__e"><view class="msg_header"><view class="header_left"><text class="names">姓名：</text><text class="namemain">{{customer.realname}}</text></view></view><view class="msg_text"><text class="names">预算：</text><text class="namemain">{{customer.budget}}</text></view><view class="msg_text"><text class="names">桌数：</text><text class="namemain">{{customer.banquet_size}}</text></view><view class="msg_text"><text class="names">酒店：</text><text class="namemain">{{customer.hotel_text}}</text></view><view class="msg_text"><text class="names">区域：</text><text class="namemain">{{customer.zone}}</text></view></view></view></block></view>